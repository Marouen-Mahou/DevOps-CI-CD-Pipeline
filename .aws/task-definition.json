{
    "family": "marouen-task-def-postsmanagerapp-pipeline",
    "containerDefinitions": [
        {
            "name": "marouen-devops-app",
            "image": "marouenmahou/postsmanager",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "1024",
    "cpu": "512"
}